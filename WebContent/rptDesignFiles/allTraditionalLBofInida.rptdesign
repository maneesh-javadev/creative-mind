            <xml-property name="queryText"><![CDATA[select distinct l.local_body_code,l.local_body_version,l.local_body_name_english,l.local_body_name_local,l.local_body_type_code,lbt.local_body_type_name,
coalesce(ip.local_body_code,0) as parent1_code
,coalesce(zp.local_body_code,0) as parent2_code,s.state_code,s.state_name_english
 from localbody l
inner join local_body_type lbt on lbt.local_body_type_code=l.local_body_type_code and lbt.category='R' and lbt.ispartixgovt=false and l.isactive=true

inner join state s on s.state_code=l.slc and s.isactive is true

left join localbody ip on l.parent_lblc=ip.local_body_code  left join
localbody zp on ip.parent_lblc=zp.local_body_code

order by s.state_name_english,l.local_body_type_code,l.local_body_name_english ]]></xml-property>
